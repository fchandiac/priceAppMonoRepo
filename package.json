{
  "name": "price-app",
  "version": "1.0.1",
  "description": "Apps for prices",
  "author": "Felipe Chand√≠a Castillo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/price-app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/price-app/test/jest-e2e.json",
    "start:next": "npm --prefix apps/next-app run start",
    "start:api": "npm --prefix apps/api run start",
    "start": "concurrently \"npm run start:next\" \"npm run start:api\"",
    "build": "npm --prefix apps/api run build && npm --prefix apps/next-app run build",
    "build:next": "npm --prefix apps/next-app run build",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:api\"",
    "dev:next": "npm --prefix apps/next-app run dev",
    "dev:api": "npm --prefix apps/api run dev"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@npmcli/fs": "^3.1.1",
    "chartjs-adapter-moment": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "duckdb": "0.10.1",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "glob": "^11.0.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "xlsx-populate": "^1.21.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.6",
    "@mui/x-charts": "^7.13.0",
    "@mui/x-date-pickers": "^7.13.0",
    "chart.js": "^4.4.3",
    "next": "14.2.5",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-to-print": "^2.15.1",
    "recharts": "^2.12.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
